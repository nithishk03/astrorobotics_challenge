<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <link name="base_footprint"/>
    
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder radius="0.3" length="0.2" />
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0" />
            <material name="" />
        </visual>
        <collision>
            <origin xyz="0 0 0.1" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.2" />
            </geometry>
    </collision>
    <inertial>
         <mass value="5"/>
         <inertia ixx="0.1292" ixy="0.0" ixz="0.0" iyy="0.1292" iyz="0.0" izz="0.225"/>
    </inertial>
    </link>

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_footprint" />
        <child link="base_link" />
        <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </joint>

     <link name="base_scan">
        <visual>
            <geometry>
                <cylinder radius="0.29" length="0.02" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="" />
        </visual>
        <collision>
            <origin xyz="0 0 0.1" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.3" length="0.2" />
            </geometry>
    </collision>
    <inertial>
         <mass value="5"/>
         <inertia ixx="0.1292" ixy="0.0" ixz="0.0" iyy="0.1292" iyz="0.0" izz="0.225"/>
    </inertial>
     </link>

    <joint name="base_scan_joint" type="fixed">
        <parent link="base_link" />
        <child link="base_scan" />
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </joint>

    <link name="right_wheel_link">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0" />
            <material name="" />
        </visual>
        <collision>
            <geometry>
            <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0" />
    </collision>
    <inertial>
         <mass value="1"/>
         <inertia ixx="0.0027" ixy="0.0" ixz="0.0" iyy="0.0027" iyz="0.0" izz="0.005"/>
    </inertial>
    </link>


    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link" />
        <child link="right_wheel_link" />
        <origin xyz="0 -0.2 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>


    <link name="left_wheel_link">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0" />
            <material name="" />
        </visual>
         <collision>
         <geometry>
             <cylinder radius="0.1" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0" />
    </collision>
    <inertial>
         <mass value="1"/>
         <inertia ixx="0.0027" ixy="0.0" ixz="0.0" iyy="0.0027" iyz="0.0" izz="0.005"/>
    </inertial>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link" />
        <child link="left_wheel_link" />
        <origin xyz="0 0.2 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>

    <link name="caster_wheel_1">
        <visual>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="" />
        </visual>
        <collision>
      <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
         <mass value="0.5"/>
         <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
    </inertial>
    </link>

 <link name="caster_wheel_2">
        <visual>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="" />
        </visual>
        <collision>
      <geometry>
                <sphere radius="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
         <mass value="0.5"/>
         <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
    </inertial>
    </link>

    <joint name="caster_wheel_joint_1" type="fixed">
        <parent link="base_link" />
        <child link="caster_wheel_1" />
        <origin xyz="0.2 0 -0.05" rpy="0 0 0" />
    </joint>

    <joint name="caster_wheel_joint_2" type="fixed">
        <parent link="base_link" />
        <child link="caster_wheel_2" />
        <origin xyz="-0.2 0 -0.05" rpy="0 0 0" />
    </joint>
</robot>